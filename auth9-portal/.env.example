# Auth9 Portal Configuration

# Auth9 Core API URL
AUTH9_CORE_URL=http://localhost:8080

# Auth9 Portal URL (for redirect_uri)
AUTH9_PORTAL_URL=http://localhost:3000

# Portal client_id registered in auth9-core
AUTH9_PORTAL_CLIENT_ID=auth9-portal

# Session secret (generate with: openssl rand -hex 32)
SESSION_SECRET=your-session-secret-change-this

# Node environment
NODE_ENV=development
